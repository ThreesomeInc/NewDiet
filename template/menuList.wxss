.next-icon {
    position:absolute;
    top: 50%;
    right: 50rpx;
    transform: translateY(-50%);
    width: 45rpx;
    height: 45rpx;
}
.title-icon {
  width: 60rpx;
  height: 60rpx;
  position:absolute;
  top: 50%;
  left: 50rpx;
  transform: translateY(-50%);
}
/* menu */
.menu-list {
  width: 100%;
  margin-top: 50rpx;
}
.menu-item {
    position: relative;
    height: 80rpx;
    line-height: 80rpx;
    background-color: #ffffff;
    font-size: 35rpx;
    margin-bottom: 20rpx;
    box-sizing:border-box;
    padding-left: 50rpx;
}

.menu-item .menu-item-title {
    overflow:hidden;
    text-align: left;
    padding: 0 70rpx;
    box-sizing: border-box;
}