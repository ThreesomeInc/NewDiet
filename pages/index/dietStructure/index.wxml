<view class="main-container">
  <view class="tab-header">
    <view class="headerText">
      <text>{{headerText}}</text>
    </view>
    <view class="subHeader">
      <text>{{subHeader}}</text>
    </view>
  </view>

  <view class="bar-char">
    <text class="bar-chart-title">{{barChartTitle}}</text>
    <text class="bar-chart-sub-title">{{barChartSubTitle}}</text>
    <canvas 
      canvas-id="columnCanvas" 
      class="canvas" bindtouchstart="touchHandler">
    </canvas>
    <text class="bar-chart-sub-title">{{advice}}</text>
  </view>
  
</view>
