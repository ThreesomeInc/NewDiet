
.data-checkbox-view {
    width:100%;
    text-align: center;
    vertical-align: middle;
    padding-top:30rpx;
    padding-bottom: 40rpx;
}
.checkbox-group {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content:center;
  align-items: center;
  margin: 0 30rpx;
  line-height: 2.5rem;
}
.checkbox-group label {
  margin: 30rpx 20rpx;
  padding: 10rpx 30rpx;
  border: 1px solid black;
  border-radius: 30rpx;
  text-align: left;
}





